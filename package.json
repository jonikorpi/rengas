{
  "name": "scrollworld",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.13.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-firebase": "^2.2.8",
    "react-scripts": "1.1.4",
    "source-map-explorer": "^1.5.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "analyze": "yarn run build && source-map-explorer build/static/js/main.*",
