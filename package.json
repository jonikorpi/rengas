{
  "name": "rengas",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-network": "^1.0.1",
    "react-scripts": "1.0.13",
    "serve": "latest",
    "source-map-explorer": "^1.5.0",
